#include <stdio.h>
#define SUM(a, b) ((a) + (b))
#define PRODUCT(a, b) ((a) * (b))
int main() {
    int num1, num2;
    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);
    printf("Sum of %d and %d is: %d\n", num1, num2, SUM(num1, num2));
    printf("Product of %d and %d is: %d\n", num1, num2, PRODUCT(num1, num2));
    return 0;
}
